<Application x:Class="TaskTester.DesktopTester.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:TaskTester.DesktopTester.View.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibility"/>
            <converters:IconConverter x:Key="StatusIconConverter"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="VSIcons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
